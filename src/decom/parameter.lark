start: measurand ";"?

_sep{x, sep}: x (sep x)*

measurand: (parameter) | ("[" parameter "]")
    // | parameter ";" interp 
    // | parameter ";" interp ";" euc

parameter: component
    | components
    | "(" components ")" REVERSE?

components: component
    | _sep{component, "+"}
    | INT "-" INT

component: INT ":" INT "-" INT REVERSE?
    | INT ":" INT
    | INT REVERSE?

REVERSE: "R"i

%import common.INT
